@using MaximWebApp.Models;
@model List<Servicee>
<h1> Servicees

    <a class="btn btn-primary" asp-action="Create" > Create</a>
</h1>

<div class="container">

    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
    </tr>
  </thead>
  <tbody>
            @foreach (var item in Model)
            {
                
            <tr>
          
          <td>@item.Id</td>
          <td>@item.Name</td>
          <td>@item.Description</td>
          <td>
              <a class="btn btn-warning" asp-action="Update" asp-controller="Servicee" asp-route-id=@item.Id>Update</a>
                  <a class="btn btn-danger" asp-action="Delete" asp-controller="Servicee" asp-route-id=@item.Id>Delete</a>    
          </td>
            </tr>

            }


  </tbody>
</table>

</div>
